{
  "name": "nostrawl",
  "version": "0.1.7",
  "description": "A nostr library for continously retrieving events from nostr relays based on a set of filters.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node esbuild.config.js --build",
    "dev": "node esbuild.config.js --watch",
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "example:pqueue:basic": "tsx examples/pqueue/basic-example.ts",
    "example:pqueue:advanced": "tsx examples/pqueue/advanced-example.ts",
    "example:bullmq": "docker compose -f examples/bullmq/docker-compose.yaml up --build"
  },
  "dependencies": {
    "@nostr-fetch/adapter-nostr-tools": "0.13.1",
    "bullmq": "4.14.2",
    "chalk": "5.4.1",
    "dotenv": "16.3.1",
    "ioredis": "5.3.2",
    "javascript-time-ago": "2.5.9",
    "lmdb": "3.0.5",
    "nostr-fetch": "0.13.1",
    "nostr-tools": "1.17.0",
    "p-queue": "7.4.1",
    "promise-deferred": "2.0.4",
    "ramda": "0.30.1",
    "websocket-polyfill": "0.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "@types/ramda": "^0.29.10",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "3.1.1",
    "esbuild": "^0.20.0",
    "rimraf": "^5.0.5",
    "tsx": "4.19.3",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}